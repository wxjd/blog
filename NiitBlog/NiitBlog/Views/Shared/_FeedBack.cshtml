<!-- Modal -->
<div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-header header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 id="myModalLabel">反馈</h3>
    </div>
    <form method="post" id="compliantfrm">
        <div class="modal-body">
            <h2>建议或Bug</h2>
            <div class="control-group">
                <label class="control-label" for="title">主题</label>
                <div class="controls">
                    <input type="text" id="title" name="title" autocomplete="off">
                </div>
            </div>
            <div class="control-group">
                <label class="control-label" for="text">内容</label>
                <div class="controls">
                    <textarea rows="5" id="text" name="text" autocomplete="off"></textarea>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label" for="email" autocomplete="off">邮箱</label>
                <div class="controls">
                    <input type="text" id="email" name="email">
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="submit" class="btn btn-primary" id="btn_sub">提交</button>
            <button class="btn" id="btn_close" data-dismiss="modal" aria-hidden="true">关闭</button>
        </div>
    </form>
</div>
